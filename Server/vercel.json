{
    "version": 2,
    "functions": {
      "index.js": {
        "memory": 3008,
        "maxDuration": 10
      }
    },
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/index.js"
      }
    ],
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ]
  }